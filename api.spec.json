{
    "openapi": "3.0.3",
    "info": {
      "title": "Category RESTful API",
      "description": "API Spec for Category RESTful API",
      "version": "1.0.0"
    },
    "servers": [
      {
        "url": "https://localhost:3000/api"
      }
    ],
    "paths": {
        "/categories" : {
            "get" : {
                "description": "list all Categories",
                "responses": {
                    "200":{
                        "description": "Succes get all Categories",
                        "content": {
                            "application/json" : {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "code" : {
                                            "type": "number"
                                        },
                                        "status" : {
                                            "type": "string"
                                        },
                                        "data" :{
                                            "type": "array",
                                            "items": {
                                                "type": "object",
                                                "properties": {
                                                    "id" : {
                                                        "type": "number"
                                                    },
                                                    "name" :{
                                                        "type": "string"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
  }
  